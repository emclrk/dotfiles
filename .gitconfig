[alias]
	co = checkout
	st = status
	br = branch
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[push]
	default = current
[user]
	name = eay
	email = eay
[core]
	editor = vim
[init]
  defaultBranch = main
[merge]
	tool = vimdiff
